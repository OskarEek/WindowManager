<Window x:Class="WindowManager.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WindowManager"
        mc:Ignorable="d"
        Focusable="True"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Grid.Column="0" Margin="10" VerticalAlignment="Top">
            <Button Content="Start program" Width="100" Margin="0,0,0,5" Click="StartProgramButton"/>
        </StackPanel>
    </Grid>
</Window>
